<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog title="{i18n>material}: {localModel>stockMaterial}" contentHeight="100%">
		<content>
			<Table id="idStackDetailsDialog" noDataText="No Data Found"
				items="{/ZSTOCK_QTYSet}" updateFinished="updateFinishedStockTable"
				headerText="{i18n>stockDetailList}({localModel>stockTableCount})">
				<infoToolbar>
					<OverflowToolbar>
						<Title text="{i18n>material}"></Title>
					</OverflowToolbar>
				</infoToolbar>
				<ColumnListItem vAlign="Middle">
					<cells>
						<Text text="{WERKS}" />
						<Text text="{MENGE}" />
						<Text text="{A_MENGE}" />
						<Text text="{B_MENGE}" />
					</cells>
				</ColumnListItem>
				<columns>
					<Column>
						<header>
							<Text text="{i18n>supplyPlant}" />
						</header>
					</Column>
					<Column>
						<header>
							<Text text="{i18n>stock}" />
						</header>
					</Column>
					<Column>
						<header>
							<Text text="{i18n>allocatedStock}" />
						</header>
					</Column>
					<Column>
						<header>
							<Text text="{i18n>balanceStock}" />
						</header>
					</Column>
				</columns>
			</Table>
		</content>
		<endButton>
		  <Button  text="{i18n>closeBtn}" press="closeStockViewDialog"/>
		</endButton>
	  </Dialog>
</core:FragmentDefinition>
